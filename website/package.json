{
    "name": "website",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "build:dev": "doppler run -c dev -- remix vite:build",
        "build": "doppler run -- remix vite:build && pnpm copy-prisma",
        "copy-prisma": "cp ../db/schema.prisma ./build/ && doppler run -- pnpm prisma generate --schema build/schema.prisma",
        "test": "doppler run -- vitest",
        "dev": "doppler run --preserve-env -- remix vite:dev --port 3467  --host ",
        "dev:prod": "PUBLIC_SERVER_URL=https://minecord-rust-server.fly.dev pnpm dev",
        "fly": "fly -a minecord-website ",
        "deployment": "tsx scripts/deploy.ts",
        "start": "PORT=3467 doppler run -- remix-serve ./build/server/index.js"
    },
