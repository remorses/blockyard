{
    "compilerOptions": {
        "target": "ES2017",
        "module": "commonjs",
        "allowJs": true,
        "strictPropertyInitialization": false,

        "moduleResolution": "Node",
        "lib": [
            "es2022",
            "es2017",
            "es7",
            "es6"
            // "dom"
        ],
        "declaration": true,
        "declarationMap": true,

        "downlevelIteration": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "jsx": "react-jsx",
        "skipLibCheck": true
    }
}
